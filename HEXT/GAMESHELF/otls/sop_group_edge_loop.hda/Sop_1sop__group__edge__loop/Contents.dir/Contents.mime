--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/python1.parm"
Content-Type: text/plain

{
version 0.8
python	[ 0	locks=0 ]	(	"node = hou.pwd()
geo = node.geometry()

# Add code to modify contents of geo.
# Use drop down menu to select examples.
group_name = hou.node(\"..\").parm(\"group\").eval()

point_group = geo.findPointGroup(group_name)
edge_group = geo.findEdgeGroup(group_name)
prim_group = geo.findPrimGroup(group_name)
step = node.parent().parm(\"step\").eval()

override_name = node.parent().parm(\"override_name\").eval()
new_name = node.parent().parm(\"new_name\").eval()

if point_group:
    original_points = geo.globPoints(group_name)
    new_points = geo.pointLoop(original_points, hou.componentLoopType.Extended)
    
    if override_name:
        point_group = geo.createPointGroup(new_name)
    
    point_group.clear()
    point_group.add(new_points[::step])  
    
if edge_group:
    original_edges = geo.globEdges(group_name)
    new_edges = geo.edgeLoop(original_edges, hou.componentLoopType.Extended,False, False, True)

    if override_name:
        edge_group = geo.createEdgeGroup(new_name)
    
    edge_group.clear()
    edge_group.add(new_edges[::step])  

if prim_group:
    original_prims = geo.globPrims(group_name)
    new_prims = geo.primLoop(original_prims, hou.componentLoopType.Extended)
    
    if override_name:
        prim_group = geo.createPrimGroup(new_name)
        
    prim_group.clear()
    prim_group.add(new_prims[::step])  "	)
}

