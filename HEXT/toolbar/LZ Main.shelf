    <script scriptType="python"><![CDATA[add_static = 0
if kwargs['ctrlclick'] == True:
    add_static = 1

dop = hou.currentDopNet()
children =  dop.children()

for child in children:
    if child.type().name()=='output':
        inputs = child.inputs()

        gravity = child.createInputNode(0,'gravity')
        merge = gravity.createInputNode(0,'merge')
        ground = merge.createInputNode(0,'groundplane')
        
        if add_static == 1:
            static = merge.createInputNode(0,'staticobject')
            merge.setNextInput(ground)
        
        if len(inputs)>0:
            merge.setNextInput(inputs[0])
            pos = inputs[0].position()
            offset = hou.Vector2(0,-0.75)
            
            merge.setPosition(pos + 2*offset)
            gravity.setPosition(pos + 3*offset)
            child.setPosition(pos + 4*offset)
            ground.setPosition(pos + offset + hou.Vector2(-1.5,0))
            if add_static == 1:
                static.setPosition(pos + offset + hou.Vector2(-3,0))
        else: 
            offset = hou.Vector2(0,-0.75)
            pos = child.position() - offset*4            
            
            merge.setPosition(pos + 2*offset)
            gravity.setPosition(pos + 3*offset)
            ground.setPosition(pos + offset + hou.Vector2(-1.5,0))
            if add_static == 1:
                static.setPosition(pos + offset + hou.Vector2(-3,0))
        ]]></script>
